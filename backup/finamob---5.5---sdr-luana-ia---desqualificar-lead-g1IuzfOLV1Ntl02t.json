arTelefone(raw) {\n  if (!raw) return null;\n\n  // Remove tudo que não é número\n  let t = raw.toString().replace(/\\D/g, \"\");\n\n  // Garante que começa com 55\n  if (!t.startsWith(\"55\")) {\n    t = \"55\" + t;\n  }\n\n  // Agora t = 55 + DDD + número\n\n  // Se vier sem o 9 → depois de 55 tem apenas 10 dígitos\n  // 55 + DDD(2) + número(8) = 12 dígitos TOTAL\n  if (t.length === 12) {\n    const pais = t.slice(0, 2);     // 55\n    const ddd = t.slice(2, 4);      // ex: 67\n    const base = t.slice(4);        // ex: 92190178 (8 dígitos)\n    t = pais + ddd + \"9\" + base;    // insere o 9 após o DDD\n  }\n\n  // Retorna com o +\n  return \"+\" + t;\n}\n\nreturn items.map(item => {\n  const telefone = item.json.telefone;\n\n  return {\n    json: {\n      ...item.json,\n      telefonePadronizado: padronizarTelefone(telefone),\n    }\n  };\n});\n"},"type":"n8n-nodes-base.code","typeVersion":2,"position":[16,640],"id":"0137112c-5b9b-4b03-bcdb-f1b683d774cc","name":"Padronizar o telefone"}],"connections":{"trigger":{"main":[[{"node":"Padronizar o telefone","type":"main","index":0}]]},"Get many database pages":{"main":[[{"node":"Update a database page","type":"main","index":0}]]},"Padronizar o telefone":{"main":[[{"node":"Get many database pages","type":"main","index":0}]]}},"settings":{"executionOrder":"v1","callerPolicy":"workflowsFromSameOwner","errorWorkflow":"Cem9y3zmcggUQhrp"},"staticData":{"node:Stripe Trigger":{"webhookId":"we_1S45HZRuKlw6o0Hg4nRNx3O2","webhookEvents":["customer.subscription.created"],"webhookSecret":"whsec_u9SDEbFzkZY1tJcmw28bdoOkPw1RIb4p"}},"meta":{"templateCredsSetupCompleted":true},"pinData":{"trigger":[{"json":{"telefone":"556792190178","motivo":"Em contenção de custos devido à cirurgia no joelho e recuperação em andamento, por isso não tem interesse em nossos produtos no momento."}}]},"versionId":"9f9b8459-fb29-445f-91ce-64bf6674258a","triggerCount":1,"tags":[]}